{% if primary %}
  <h2 class="visually-hidden">{{ 'Primary tabs'|t }}</h2>
  <ul class="{{ bem('tabs', ['primary']) }}">{{ primary }}</ul>
{% endif %}
{% if secondary %}
  <h2 class="visually-hidden">{{ 'Secondary tabs'|t }}</h2>
  <ul class="{{ bem('tabs', ['secondary']) }}">{{ secondary }}</ul>
{% endif %}
