{#
/**
 * @file
 * Theme override to display a region.
 *
 * @see template_preprocess_region()
 */
#}
{% set attributes = attributes.setAttribute('id', 'sidebar-first') %}
{% set attributes = attributes.addClass('region') %}
{% set attributes = attributes.addClass('region--sidebar-first') %}
{% if content %}
  <aside{{ attributes }}>
    {{ content }}
  </aside>
{% endif %}